  "bugs": {
    "url": "https://github.com/JazzoLopez/SPC-integradora/issues"
  },
  "homepage": "https://github.com/JazzoLopez/SPC-integradora#readme",
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "bcrypt": "^5.1.1",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
